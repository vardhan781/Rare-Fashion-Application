{
  "expo": {
    "name": "RareFashion",
    "slug": "rare-fashion-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/fashion.png",
    "scheme": "rarefashion",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/fashion.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "backendUrl": "https://rare-fashion-backend.vercel.app",
      "eas": {
        "projectId": "440d40dd-a083-4169-bbb7-9084d7543a85"
      }
    },
    "android": {
      "package": "com.rarefashion.rarefashion",
      "versionCode": 1,
      "permissions": [
        "INTERNET"
      ],
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/fashion.png",
        "backgroundColor": "#fbeaea"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "newArchEnabled": true,
    "owner": "vardhan_sinh",
    "plugins": [
      "expo-font"
    ]
  }
}
